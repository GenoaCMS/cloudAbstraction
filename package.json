{
  "name": "@genoacms/cloudabstraction",
  "version": "0.4.0",
  "description": "Types for abstract cloud for GenoaCMS",
  "author": {
    "name": "Filip Holčík",
    "email": "filip.holcik.official@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GenoaCMS/cloudAbstraction.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GenoaCMS/cloudAbstraction/issues"
  },
  "homepage": "https://github.com/GenoaCMS/cloudAbstraction#readme",
  "publishConfig": {
    "directory": "./dist"
  },
  "files": [
    "src",
    "dist",
    "test"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": "./dist/index.d.ts",
    "./authorization": {
      "import": "./dist/services/authorization/index.js",
      "require": "./dist/services/authorization/index.js",
      "types": "./dist/services/authorization/index.d.ts"
    },
    "./clientAuthentication": {
      "import": "./dist/services/clientAuthentication/index.js",
      "require": "./dist/services/clientAuthentication/index.js",
      "types": "./dist/services/clientAuthentication/index.d.ts"
    },
    "./database": {
      "import": "./dist/services/database/index.js",
      "require": "./dist/services/database/index.js",
      "types": "./dist/services/database/index.d.ts"
    },
    "./storage": {
      "import": "./dist/services/storage/index.js",
      "require": "./dist/services/storage/index.js",
      "types": "./dist/services/storage/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "ajv": "^8.12.0",
    "eslint": "^8.52.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "typescript": "^5.2.2",
    "vitest": "^1.1.0"
  }
}
